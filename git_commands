sudo yum install git -y 

mkdir git_folder

cd git_folder

git init

git config --global user.name "kavitha" 

git config --global user.email "kavitha@gmail.com" 

git config --global --list 

git add <file_name> 

git add .

git add --all 

gti commit -m "commit-message" 

git push -u origin main 

git push  

git status 

git log 

git log --oneline 

git rm --cache <file-name> 

git reset --hard 

git reset <file-name> 

git show 

git log -1 

git clone <ur> 

git pull <url> 

git branch 

git branch -M <branch-name> 

git checkout <branch-name> 

git checkout -b <new-branch-name>

git branch -d <branch-name> 

git merge <branch-name>

git diff 

git stash 


